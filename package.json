{
  "name": "anselm",
  "displayName": "Anselm",
  "description": "Qualitative coding of Markdown",
  "version": "0.4.0",
  "publisher": "edsu",
  "icon": "images/anselm.png",
  "repository": "https://github.com/edsu/anselm",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:anselm.code",
    "onView:anselm.codeTree"
  ],
  "main": "./src/anselm.js",
  "contributes": {
    "commands": [
      {
        "command": "anselm.code",
        "title": "Anselm: Code"
      },
      {
        "command": "anselm.refresh",
        "title": "Anselm: Refresh Code View"
      }
    ],
    "keybindings": [
      {
        "command": "anselm.code",
        "key": "cmd+z cmd+c",
        "when": "editorTextFocus"
      },
      {
        "command": "anselm.refresh",
        "key": "cmd+z cmd+r",
        "when": "editorFocus"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "anselm.codeTree",
          "name": "Codes"
        }
      ]
    }
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "eslint": "^6.6.0",
    "glob": "^7.1.5",
    "mocha": "^10.2.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "walk": "^2.3.14"
  }
}
